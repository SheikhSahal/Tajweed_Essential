@model List<WebApplication1.Models.AP_Menu>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Dashboard.cshtml";
}
<div class="main-content">
    <div class="section__content section__content--p30">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <!-- DATA TABLE -->
                    <h3 class="title-5 m-b-35">All courses</h3>
                    <div class="table-data__tool">
                        <div class="table-data__tool-left" style="visibility:hidden;">
                            <div class="rs-select2--light rs-select2--md">
                                <select class="js-select2" name="property">
                                    <option selected="selected">All Properties</option>
                                    <option value="">Option 1</option>
                                    <option value="">Option 2</option>
                                </select>
                                <div class="dropDownSelect2"></div>
                            </div>
                            <div class="rs-select2--light rs-select2--sm">
                                <select class="js-select2" name="time">
                                    <option selected="selected">Today</option>
                                    <option value="">3 Days</option>
                                    <option value="">1 Week</option>
                                </select>
                                <div class="dropDownSelect2"></div>
                            </div>
                            <button class="au-btn-filter">
                                <i class="zmdi zmdi-filter-list"></i>filters
                            </button>
                        </div>
                        <div class="table-data__tool-right">
                            <a href="@Url.Action("Index","New_Course")" class="au-btn au-btn-icon au-btn--green au-btn--small">
                                <i class="zmdi zmdi-plus"></i>Add course
                            </a>
                        </div>
                    </div>
                    <div class="table-responsive table-responsive-data2">
                        <table class="table table-data2">
                            <thead>
                                <tr>
                                    <th>S/No.</th>
                                    <th>Course Name</th>
                                    <th>Teacher Name</th>
                                    <th>End Date</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @{int index = 1;}
                                @foreach (var s in ViewBag.batchlist as List<WebApplication1.Models.Batch_list>)
                                {
                                    <tr class="tr-shadow">
                                        <td>@index</td>
                                        <td>@s.Batch_name</td>
                                        <td>@s.Teach_name</td>
                                        <td>@s.bh_end_date.ToString("dd-MMM-yyyy")</td>
                                        <td>
                                            <label for="checkbox1" class="form-check-label ">
                                                <input type="checkbox" id="hide" name="hide" value="Y" class="form-check-input">Hide
                                            </label>
                                        </td>
                                       
                                        @*<td>
                                                @{ var current_date = DateTime.Now.Date;}
                                                @if (s.bh_end_date.Date >= current_date)
                                                {
                                                    <div class="table-data-feature">
                                                        <button class="item" data-toggle="tooltip" data-placement="top" title="Edit" onclick="location.href='@Url.Action("updateCourse", "Course_List", new { id = s.Bh_id })'">
                                                            <i class="zmdi zmdi-edit"></i>
                                                        </button>
                                                        <button class="item" data-toggle="tooltip" data-placement="top" title="Delete" onclick="location.href='@Url.Action("DeleteRecord", "Course_List", new { id = s.Bh_id })'">
                                                            <i class="zmdi zmdi-delete"></i>
                                                        </button>
                                                    </div>
                                                }
                                            </td>*@
                                        
                                    </tr>
                                    index++;
                                }
                            </tbody>
                        </table>
                    </div>
                    <!-- END DATA TABLE -->
                </div>
            </div>
        </div>
    </div>
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<script>
    $(document).ready(function () {
        alert("working");
    })
</script>