@model List<WebApplication1.Models.AP_Menu>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Dashboard.cshtml";
}

<div class="main-content">
    <div class="section__content section__content--p30">
        <div class="container-fluid">
            <div class="row m-t-30">
                <div class="col-md-12">
                    <!-- DATA TABLE-->
                    <div class="table-responsive m-b-40">
                        <table class="table table-borderless table-data3">
                            <thead>
                                <tr>
                                    <th>S/No.</th>
                                    <th>User Name</th>
                                    <th>E-mail</th>
                                    <th>Contact No.</th>
                                    <th>Status</th>
                                    <th></th>
                                </tr>
                            </thead>
                            @{ int inde = 1;}
                            <tbody>
                                @foreach(var userdata in ViewBag.userdata as List<WebApplication1.Models.Registor>)
                                {
                                <tr>
                                    <td>@inde</td>
                                    <td>@userdata.Full_Name</td>
                                    <td>@userdata.email</td>
                                    <td>@userdata.User_contact</td>
                                    <td>@userdata.User_status</td>
                                    <td>
                                        <div class="table-data-feature">
                                            @if(userdata.User_status == "Waiting")
                                            {
                                            <button class="item" data-toggle="tooltip" data-placement="top" title="Approve" onclick="location.href='@Url.Action("Approved", "User_Status", new { id =userdata.User_id})'">
                                                <i class="zmdi zmdi-check"></i>
                                            </button>
                                            }
                                            <button class="item" data-toggle="tooltip" data-placement="top" title="Delete" onclick="location.href='@Url.Action("DeleteUser", "User_Status", new { id =userdata.User_id})'">
                                                <i class="zmdi zmdi-delete"></i>
                                            </button>
                                        </div>
                                    </td>
                                </tr>
                                    inde++;
                                }
                            </tbody>
                        </table>
                    </div>
                    <!-- END DATA TABLE-->
                </div>
            </div>
        </div>
    </div>
</div>
